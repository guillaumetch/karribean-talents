{% block javascripts %}
    <script src="https://unpkg.com/scrollreveal/dist/scrollreveal.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function(){
            $(".button-collapse").sideNav();
            $('.parallax').parallax();
            $('.carousel').carousel();
            $('.carousel-arts').carousel({dist:-50});
            $('ul.tabs').tabs();
            $('.next').click(function() {
                $('.carousel').carousel('next');
            });

            $('.prev').click(function() {
                $('.carousel').carousel('prev');
            });

            $('.modal').modal();

            $('.btn-read').on('click',function () {
                $(this).hide();
                var parents = $(this).parents('.card.timeline-content');
                parents.find('p').removeClass('truncate');
                parents.find('i').removeClass('hide');
            });

            $('.btn-close').on('click',function () {
                $(this).addClass('hide');
                var parents = $(this).parents('.card.timeline-content');
                parents.find('p').addClass('truncate');
                parents.find('.btn-read').show();
            });

            $('.fos_user_registration_register').submit(function() {
                $this = $(this);
                $.post($(this).attr('action'), $(this).serialize(), function(data) {
                    if (data.status === 1) {
                        location.href = data.url;
                    }
                    else{
                        var divErrors = $('#auth').find('.errors');
                        divErrors.html('');

                        for (var e in data.errors) {
                            if (data.errors[e] === 'password.mismatch') {
                                divErrors.append("Les mots de passe ne correspondent pas !<br />");
                            }
                            else {
                                divErrors.append("L'adresse e-mail est déjà utilisée !");
                            }
                        }
                    }
                });
                return false;
            });

            $('.fos_user_login').submit(function() {
                $this = $(this);
                $.post($(this).attr('action'), $(this).serialize(), function(data) {
                    if(data.status === 1) {
                        location.href = data.url;
                    }
                    else {
                        var divErrors = $('#auth').find('.errors');
                        divErrors.html(data.error);
                    }
                });
                return false;
            });

            // Changing the defaults
            window.sr = ScrollReveal({ reset: true });
//            sr.reveal('.map', { duration: 2000 });
            sr.reveal('.artiste', { duration: 3000 });
            sr.reveal('.visiteur', { duration: 3000 });

        });
    </script>
